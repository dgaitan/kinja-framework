{
    "name": "dgaitan/kinja-framework",
    "description": "A Basic Framework to write better WP Plugins.",
    "autoload": {
        "psr-4": {
            "Kinja\\Framework\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nesbot/carbon": "2.62.1",
        "tightenco/collect": "8"
    },
    "require-dev": {
        "phpunit/phpunit": "9.1",
        "brain/monkey": "2.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}